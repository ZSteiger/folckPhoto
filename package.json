{
  "name": "folckphoto",
  "version": "1.0.0",
  "description": "Folck Photo",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start -p $PORT",
    "heroku-postbuild": "npm run build",
    "test": "SET NODE_ENV=test&& jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZSteiger/folckphoto.git"
  },
  "author": "Zachary Steiger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZSteiger/folckphoto/issues"
  },
  "homepage": "https://github.com/ZSteiger/folckphoto#readme",
  "dependencies": {
    "acorn": "^6.4.1",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "next": "^9.1.6",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-adopt": "^0.6.0",
    "react-dom": "^16.8.2",
    "react-photo-gallery": "^6.3.2",
    "react-transition-group": "^2.6.0",
    "styled-components": "^4.1.3",
    "waait": "^1.0.4"
  },
  "devDependencies": {
    "babel-jest": "^24.3.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "casual": "^1.6.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.3.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".*": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
